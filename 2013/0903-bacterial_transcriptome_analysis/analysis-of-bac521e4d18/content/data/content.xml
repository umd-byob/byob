    <object id="88" type="text" x="15678.53389454855" y="-8042.404639027981" r="0" s="11.490952588942404" class="body">
      <height>178.1015625</height>
      <width>554.9996293368029</width>
      <p listType="bullet" indent="1">
        <text><![CDATA[Use HTSeq to count the number of reads in each gene (needs data in SAM format)]]></text>
      </p>
      <p listType="bullet" indent="2">
        <text><![CDATA[samtools view alignment.sorted.bam > alignment.sorted.sam ]]></text>
      </p>
      <p listType="bullet" indent="2">
        <text><![CDATA[htseq-count -s yes(/no) -t gene -i locus_tag alignment.sorted.sam genome.gff > reads.txt]]></text>
      </p>
      <layout>
        <layout-element role="body" parent-id="79"/>
      </layout>
    </object>
