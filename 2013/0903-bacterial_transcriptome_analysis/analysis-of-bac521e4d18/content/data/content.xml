    <object id="77" type="text" x="6425.4229425151425" y="-15359.608219210333" r="0" s="12.64004784783665" class="body">
      <height>59.3671875</height>
      <width>390.9237784827681</width>
      <p align="center">
        <text><![CDATA[SAM file needs to be converted to BAM, sorted, and indexed]]></text>
      </p>
      <layout>
        <layout-element role="body" parent-id="71"/>
      </layout>
    </object>
    <object id="78" type="text" x="6229.998553323833" y="-13624.314423389227" r="0" s="12.64004784783665" class="strong">
      <height>172.8</height>
      <width>-464.87999999999994</width>
      <p listType="bullet" indent="1">
        <text><![CDATA[Convert only mapped reads to BAM]]></text>
      </p>
      <p listType="bullet" indent="2">
        <text><![CDATA[samtools view -bSF 4 alignment.sam > alignment.bam]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text><![CDATA[Sort the reads by position in the genome]]></text>
      </p>
      <p listType="bullet" indent="2">
        <text><![CDATA[samtools sort alignment.bam alignment.sorted]]></text>
      </p>
      <p listType="bullet" indent="1">
        <text><![CDATA[Prepare indexing file for fast access]]></text>
      </p>
      <p listType="bullet" indent="2">
        <text><![CDATA[samtools index alignment.sorted.bam]]></text>
      </p>
    </object>
